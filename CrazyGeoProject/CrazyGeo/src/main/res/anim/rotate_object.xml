<set xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:fromDegrees="0"
        android:toDegrees="-30"
        android:pivotX="50%"
        android:pivotY="50%"
        android:duration="3000"
        android:startOffset="1000" />
    <rotate
            android:fromDegrees="0"
            android:toDegrees="30"
            android:pivotX="50%"
            android:pivotY="50%"
            android:duration="3000"
            android:startOffset="4000" />

</set>